def print_board(board):
    for row in board:
        print(" ".join(row))

def initialize_board():
    board = [[" " for _ in range(8)] for _ in range(8)]
    for i in range(8):
        if i % 2 == 0:
            board[i][1] = "B"
            board[i][5] = "W"
            board[i][7] = "W"
        else:
            board[i][0] = "B"
            board[i][2] = "B"
            board[i][6] = "W"
    return board
